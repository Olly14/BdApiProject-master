{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },

  "AppSettings": {
    "SiteTitle": "Bd Api App",
    "CacheDbResults": true
  },


  "Data": {
    "DefaultConnection": {
      // Use a shared (and running) LocalDB database when executing in IIS e.g.
      // "Server=(localdb)\\.\\IIS_DB;Database=MusicStore;Trusted_Connection=False;MultipleActiveResultSets=true;User ID=iis_login;Password=********"
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=MusicStore;Trusted_Connection=True;MultipleActiveResultSets=true;Connect Timeout=30;"
    }
  },



  "ConnectionStrings": {
    "BdConnectionString": "server=(localdb)\\MSSQLLocalDB;database=BarAndDagDb;Trusted_Connection=true",
    "UserIdentityDbConnectionString": "server=(localdb)\\MSSQLLocalDB;database=IdentityDb;Trusted_Connection=true"
  },

  "AllowedHosts": "*"
}
